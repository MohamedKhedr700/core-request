{
    "name": "raid/core-request",
    "description": "Raid Core Request Package",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "keywords": [
        "laravel",
        "raid",
        "core",
        "validation",
        "request",
        "form-request"
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Raid\\Core\\Request\\": "src"
        }
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint --config pint.json "
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raid\\Core\\Request\\Providers\\RequestServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "mohamed_khedr",
            "email": "mohamedkhedr700@gmail.com"
        }
    ]
}
